{
  "name": "MoonGun Dark Neovim",
  "type": "dark",
  "colors": {
    "focusBorder": "#a394f033",
    "foreground": "#bdbdbd",
    "errorForeground": "#ff0000",
    "widget.shadow": "#0f0f0f",
    "selection.background": "#3d375e7f",
    "titleBar.activeBackground": "#121016",
    "titleBar.border": "#000000",
    "titleBar.inactiveBackground": "#2d2b38",
    "statusBar.background": "#121016",
    "statusBar.foreground": "#adacae",
    "statusBar.border": "#000000",
    "statusBar.debuggingForeground": "#15141b",
    "statusBar.debuggingBackground": "#a19c77",
    "statusBarItem.activeBackground": "#1cecff",
    "dropdown.background": "#15141b",
    "dropdown.border": "#3b334b",
    "dropdown.foreground": "#d7d7d7",
    "input.background": "#15141b",
    "input.border": "#3b334b",
    "input.foreground": "#d7d7d7",
    "input.placeholderForeground": "#af8aff7f",
    "inputOption.activeBorder": "#1cecff",
    "button.background": "#54bec5",
    "button.foreground": "#15141b",
    "button.hoverBackground": "#49c29a",
    "list.activeSelectionBackground": "#2e2b38",
    "list.activeSelectionForeground": "#1cecff",
    "list.focusBackground": "#3b334b",
    "list.focusForeground": "#1cecff",
    "list.highlightForeground": "#1cecff",
    "list.hoverBackground": "#3b334b",
    "list.hoverForeground": "#bdbdbd",
    "list.inactiveSelectionBackground": "#2e2b38",
    "list.inactiveSelectionForeground": "#1cecff",
    "editor.background": "#15141b",
    "editor.foreground": "#bdbdbd",
    "editorLineNumber.foreground": "#a394f033",
    "editorCursor.foreground": "#1cecff",
    "editor.selectionBackground": "#1cecff28",
    "editor.selectionHighlightBackground": "#3d375e7f",
    "editor.inactiveSelectionBackground": "#3d375e7f",
    "editor.wordHighlightBackground": "#3d375e7f",
    "editor.wordHighlightStrongBackground": "#3d375e7f",
    "editor.findMatchBackground": "#3d375e7f",
    "editor.findMatchHighlightBackground": "#3d375e7f",
    "editor.findRangeHighlightBackground": "#3d375e7f",
    "editor.lineHighlightBackground": "#1cecff28",
    "editor.lineHighlightBorder": "#a394f000",
    "editor.stackFrameHighlightBackground": "#353424",
    "editorInlayHint.background": "#2e2b38",
    "editorInlayHint.foreground": "#d7d7d7",
    "editorLink.activeForeground": "#1cecff",
    "editorWhitespace.foreground": "#000000",
    "editorBracketMatch.border": "#1cecff",
    "editorError.foreground": "#ff0000",
    "editorError.border": "#ffffff00",
    "editorWarning.foreground": "#6f78c7",
    "editorWarning.border": "#ffffff00",
    "editorGutter.modifiedBackground": "#6f78c7",
    "editorGutter.addedBackground": "#54bec5",
    "editorGutter.deletedBackground": "#ff0000",
    "editorWidget.background": "#121016",
    "editorWidget.border": "#000000",
    "editorSuggestWidget.background": "#121016",
    "editorSuggestWidget.border": "#000000",
    "editorSuggestWidget.foreground": "#d7d7d7",
    "editorSuggestWidget.highlightForeground": "#54bec5",
    "editorSuggestWidget.selectedBackground": "#4d4d4d",
    "editorHoverWidget.background": "#121016",
    "editorHoverWidget.border": "#000000",
    "editorGroup.border": "#000000",
    "editorGroup.dropBackground": "#3d375e7f",
    "editorGroupHeader.tabsBackground": "#15141b",
    "editorGroupHeader.tabsBorder": "#000000",
    "tab.activeBackground": "#00000000",
    "tab.activeForeground": "#54bec5",
    "tab.border": "#000000",
    "tab.inactiveBackground": "#15141b",
    "tab.inactiveForeground": "#2c6474",
    "tab.activeBorderTop": "#54bec5",
    "panel.border": "#000000",
    "panelTitle.activeBorder": "#54bec5",
    "panelTitle.activeForeground": "#d7d7d7",
    "activityBar.background": "#15141b",
    "activityBar.foreground": "#54bec5",
    "activityBar.inactiveForeground": "#525156",
    "activityBar.border": "#000000",
    "activityBarBadge.background": "#1cecff",
    "activityBarBadge.foreground": "#15141b",
    "activityBar.activeBorder": "#1cecff",
    "badge.foreground": "#15141b",
    "badge.background": "#1cecff",
    "sideBar.background": "#110f18",
    "sideBar.foreground": "#d7d7d7",
    "sideBar.border": "#000000",
    "sideBarTitle.foreground": "#adacae",
    "sideBarSectionHeader.background": "#15141b",
    "sideBarSectionHeader.foreground": "#adacae",
    "progressBar.background": "#54bec5",
    "scrollbar.shadow": "#15141b",
    "scrollbarSlider.activeBackground": "#3d375e7f",
    "scrollbarSlider.background": "#a394f033",
    "scrollbarSlider.hoverBackground": "#a394f033",
    "gitDecoration.modifiedResourceForeground": "#6f78c7",
    "gitDecoration.deletedResourceForeground": "#ff0000",
    "gitDecoration.untrackedResourceForeground": "#54bec5",
    "gitDecoration.ignoredResourceForeground": "#4d4d4d",
    "gitDecoration.conflictingResourceForeground": "#1cecff",
    "diffEditor.insertedTextBackground": "#00d89023",
    "diffEditor.removedTextBackground": "#ff474720",
    "tree.indentGuidesStroke": "#4d4d4d"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6370"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage",
        "variable.language.super",
        "variable.language.this",
        "variable.language.special"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "keyword.other.unit",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "punctuation.definition.decorator",
        "punctuation.decorator",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Type",
      "scope": [
        "meta.type.annotation",
        "meta.type.parameters",
        "support.type.primitive",
        "support.type.builtin",
        "support.type.exception"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "punctuation.accessor",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.assignment",
        "keyword.operator.relational",
        "keyword.operator.ternary",
        "keyword.operator.arithmetic",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.type",
        "keyword.operator.optional",
        "keyword.operator.combinator",
        "keyword.operator.pattern",
        "keyword.operator.or",
        "keyword.operator.quantifier",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "keyword.operator.type.annotation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "entity.name", "support.variable"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Object key",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#98C379"
      }
    },

    // C++
    {
      "name": "C++ Keywords",
      "scope": [
        "punctuation.definition.directive.cpp",
        "keyword.other.using.directive.cpp"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "C++ Scopes",
      "scope": [
        "entity.name.scope-resolution.parameter.cpp",
        "entity.name.scope-resolution.cpp",
        "entity.name.scope-resolution.function.call.cpp",
        "entity.name.scope-resolution.template.call.cpp",
        "entity.name.scope-resolution.function.definition.cpp"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },

    // CSS/SASS
    {
      "name": "CSS/SASS keyword",
      "scope": [
        "punctuation.definition.keyword.css",
        "punctuation.definition.keyword.scss",
        "keyword.operator.logical.and.media.css",
        "keyword.operator.logical.and.media.scss",
        "keyword.operator.logical.scss"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "CSS/SASS constant",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.vendored.property-value.css",
        "support.constant.color.w3c-standard-color-name.css",
        "support.constant.font-name.css",
        "punctuation.definition.constant.css",
        "entity.other.attribute-name.css",
        "entity.other.attribute-name.attribute.scss"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "CSS/SASS operator",
      "scope": ["keyword.operator.css", "keyword.operator.scss"],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "CSS/SASS id + class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "CSS/SASS pseudo",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "CSS/SASS variable",
      "scope": ["meta.property-value.css"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "CSS/SASS key",
      "scope": [
        "support.type.map.key.scss",
        "entity.other.keyframe-offset.css",
        "meta.property-name.css",
        "entity.other.attribute-name.scss",
        "support.type.property-name.media.css"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "CSS/SASS string",
      "scope": ["meta.attribute-selector.scss"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "CSS/SASS punctuation",
      "scope": ["variable.parameter.url.css", "variable.parameter.url.scss"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },

    // GraphQL
    {
      "name": "GraphQL comment",
      "scope": ["punctuation.whitespace.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6370"
      }
    },
    {
      "name": "GraphQL type",
      "scope": [
        "support.type.graphql",
        "support.type.location.graphql",
        "support.type.enum.graphql",
        "entity.scalar.graphql"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "GraphQL operator",
      "scope": [
        "punctuation.or.graphql",
        "keyword.operator.nulltype.graphql",
        "punctuation.assignment.graphql"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },

    // HTML/XML
    {
      "name": "HTML/XML attribute",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.localname.xml",
        "entity.other.attribute-name.namespace.xml"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D19A66"
      }
    },

    // JS/TS
    {
      "name": "JSX attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D19A66"
      }
    },
    {
      "name": "JS built-ins",
      "scope": [
        "support.constant.json.js",
        "support.constant.json.jsx",
        "support.constant.json.ts",
        "support.constant.json.tsx",
        "support.constant.math.js",
        "support.constant.math.jsx",
        "support.constant.math.ts",
        "support.constant.math.tsx"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "JS modules",
      "scope": [
        "support.type.object.module.js",
        "support.type.object.module.jsx",
        "support.type.object.module.ts",
        "support.type.object.module.tsx"
      ],
      "settings": {
        "foreground": "#51B4C3"
      }
    },
    {
      "name": "JSX bracket",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.end.tsx"
      ],
      "settings": {
        "foreground": "#BE5046"
      }
    },
    {
      "name": "JS constants",
      "scope": [
        "variable.other.constant.js",
        "variable.other.constant.jsx",
        "variable.other.constant.ts",
        "variable.other.constant.tsx"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },

    // JSON
    {
      "name": "JSON key",
      "scope": [
        "string.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },

    // Markdown
    {
      "name": "Markdown heading",
      "scope": ["punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C678DD"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Markdown string",
      "scope": [
        "punctuation.definition.raw.markdown",
        "markup.inline.raw.string.markdown",
        "punctuation.definition.markdown",
        "fenced_code.block.language.markdown"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Markdown list",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Markdown blockquote",
      "scope": [
        "markup.quote.markdown",
        "punctuation.definition.quote.begin.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown links",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Markdown code block language",
      "scope": ["fenced_code.block.language.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },

    // Python
    {
      "name": "Python operator",
      "scope": [
        "keyword.operator.python",
        "keyword.operator.unpacking.arguments.python",
        "punctuation.separator.period.python"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Python keyword",
      "scope": ["keyword.operator.logical.python"],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "Python type",
      "scope": ["support.type.python"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Python source",
      "scope": ["source.python"],
      "settings": {
        "foreground": "#E06C75"
      }
    },

    // Regex

    // SQL
    {
      "name": "SQL names",
      "scope": [
        "constant.other.table-name.sql",
        "constant.other.database-name.sql"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "SQL math",
      "scope": ["keyword.operator.math.sql", "keyword.operator.star.sql"],
      "settings": {
        "foreground": "#56B6C2"
      }
    },

    // Toml
    {
      "name": "Toml key",
      "scope": [
        "keyword.key.toml",
        "entity.other.attribute-name.table.toml",
        "entity.other.attribute-name.table.array.toml"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },

    // Yaml
    {
      "name": "Yaml string multi line",
      "scope": [
        "keyword.control.flow.block-scalar.folded.yaml",
        "keyword.control.flow.block-scalar.literal.yaml",
        "storage.modifier.chomping-indicator.yaml"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    }
  ]
}